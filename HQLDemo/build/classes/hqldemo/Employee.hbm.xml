<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="hqldemo.Employee" table="WORKERS">
        
        <id name="empcode" type="int" column="w_id">
            <!--<generator class="native"/>-->
        </id>
        <property name="ename" column="w_name" type="string"/>
        <property name="esal" column="w_sal" type="double"/>
        
    </class>
</hibernate-mapping>

